<?xml version="1.0"?>

<Form Width="320" Height="240" Caption="Vario Configuration">
  <SymbolButton Name="cmdNext" Caption="&gt;" X="35" Y="160" Width="35" Height="28" OnClick="OnNextClicked" />
  <SymbolButton Name="cmdPrev" Caption="&lt;" X="0" Y="160" Width="35" Height="28" OnClick="OnPrevClicked" />
  <Button Name="cmdClose" Caption="Close" X="5" Y="190" Width="60" Height="28" />
  <Button Name="cmdDemo" Caption="Demo" X="5" Y="60" Width="60" Height="28" OnClick="OnDemoClicked" />
  <Button Name="cmdSave" Caption="Save" X="5" Y="90" Width="60" Height="28" OnClick="OnSaveClicked" />

  <Tabbed Name="tabbed" X="70" Y="0" Width="246" Height="222">
    <Panel Name="frmHardware">
      <Edit Name="prpHasPressureTE" Caption="TE port" X="2" Width="246" Height="22" CaptionWidth="150" Help="Whether the Total Energy port is connected.">
        <DataField DataType="boolean"/>
      </Edit>
    
      <Edit Name="prpHasPressurePitot" Caption="Pitot port" X="2" Width="246" Height="22" CaptionWidth="150" Help="Whether the Pitot port is connected.">
        <DataField DataType="boolean"/>
      </Edit>
    
      <Edit Name="prpHasPressureStatic" Caption="Static port" X="2" Width="246" Height="22" CaptionWidth="150" Help="Whether the Static port is connected.">
        <DataField DataType="boolean"/>
      </Edit>
    
      <Edit Name="prpHasPressureStall" Caption="Stall port" X="2" Width="246" Height="22" CaptionWidth="150" Help="Whether the Stall pressure port is connected.">
        <DataField DataType="boolean"/>
      </Edit>
    
      <Edit Name="prpHasAccelerometer" Caption="Accelerometer" X="2" Width="246" Height="22" CaptionWidth="150" Help="Whether the internal accelerometer is used.  Only change this if the accelerometer has malfunctioned or the instrument cannot be installed with correct alignment.">
        <DataField DataType="boolean"/>
      </Edit>
    
      <Edit Name="prpHasTemperature" Caption="Temperature" X="2" Width="246" Height="22" CaptionWidth="150" Help="Whether a temperature and humidity sensor is installed.  Set to 0 to disable, 255 to enable auto-detect; otherwise the 1Wire device ID can be specified.">
        <DataField DataType="enum"/>
      </Edit>
    
      <Edit Name="prpBaudRateA" Caption="Baud rate Vega" X="2" Width="246" Height="22" CaptionWidth="150" Help="Baud rate of serial device connected to Vega port X1.  Use this as necessary when using a third party GPS or data-logger instead of FLARM.  If FLARM is connected the baud rate will be fixed at 38400.  For OzFLARM, the value can be set to 19200.">
        <DataField DataType="enum"/>
      </Edit>
    
      <Edit Name="prpFlarmConnected" Caption="FLARM connected" X="2" Width="246" Height="22" CaptionWidth="150" Help="Enable detection of FLARM.  Disable only if FLARM is not used or disconnected.">
        <DataField DataType="boolean"/>
      </Edit>
    
      <Edit Name="prpEnablePDASupply" Caption="PDA power" X="2" Width="246" Height="22" CaptionWidth="150" Help="Enable output of +5V power supply for PDA etc. at Vega connector~X2.  If Vega is connected to Altair, this should be set to False.">
        <DataField DataType="boolean"/>
      </Edit>
    </Panel>
  
    <Panel Name="frmCalibration">
      <Edit Name="prpCalibrationTEProbe" Caption="TE probe cal" X="2" Width="246" Height="22" CaptionWidth="150" Help="Calibration factor applied to total energy probe to obtain TE vario.">
        <DataField DataType="double" DisplayFormat="%.0f %%" EditFormat="%.0f" Min="50" Max="150" Step="1"/>
      </Edit>
    
      <Edit Name="prpTotalEnergyMixingRatio" Caption="TE mixing" X="2" Width="246" Height="22" CaptionWidth="150" Help="Proportion of TE probe value used in total energy mixing with pitot/static total energy.">
        <DataField DataType="double" DisplayFormat="%.0f/8" EditFormat="%.0f" Min="0" Max="8" Step="1"/>
      </Edit>
    
      <Edit Name="prpCalibrationAirSpeed" Caption="ASI cal." X="2" Width="246" Height="22" CaptionWidth="150" Help="Calibration factor applied to measured airspeed to obtain indicated airspeed.">
        <DataField DataType="double" DisplayFormat="%.0f %%" EditFormat="%.0f" Min="0" Max="200" Step="1"/>
      </Edit>
    
      <Edit Name="prpCalibrationTEStatic" Caption="TE static cal." X="2" Width="246" Height="22" CaptionWidth="150" Help="Calibration factor applied to static pressure used in total energy calculation.">
        <DataField DataType="double" DisplayFormat="%.0f %%" EditFormat="%.0f" Min="0" Max="200" Step="1"/>
      </Edit>
    
      <Edit Name="prpCalibrationTEDynamic" Caption="TE dynamic cal." X="2" Width="246" Height="22" CaptionWidth="150" Help="Calibration factor applied to dynamic pressure used in total energy calculation.">
        <DataField DataType="double" DisplayFormat="%.0f %%" EditFormat="%.0f" Min="0" Max="200" Step="1"/>
      </Edit>
    </Panel>
  
    <Panel Name="frmAudioModes">
      <Edit Name="prpToneClimbComparisonType" Caption="Climb fast trigger" X="2" Width="246" Height="22" CaptionWidth="150" Help="Comparison method used to detect climb states (HIGH/LOW).&#10;[NONE]: State LOW disabled&#10;[MACCREADY]: State High if gross vario is greater than MacCready setting.&#10;[AVERAGE]: State HIGH if gross vario value is greater than average gross vario value.">
        <DataField DataType="enum"/>
      </Edit>
  
      <Edit Name="prpToneCruiseLiftDetectionType" Caption="Cruise lift trigger" X="2" Width="246" Height="22" CaptionWidth="150" Help="Comparison method used to detect cruise states for switching to lift tones during cruise.&#10;[NONE]: LIFT tone disabled in cruise&#10;[RELATIVE ZERO] LIFT tone when relative vario greater than zero.&#10;[GROSS ZERO] LIFT tone when glider is climbing.&#10;[RELATIVE MC/2] LIFT tone when relative vario greater than half MC.&#10;[NET MC/2] LIFT tone when airmass velocity greater than half MC.">
        <DataField DataType="enum"/>
      </Edit>
  
      <Edit Name="prpToneAveragerVarioTimeScale" Caption="Climb averager scale" X="2" Width="246" Height="22" CaptionWidth="150" Help="Time scale used for vario averager.">
        <DataField DataType="enum"/>
      </Edit>
  
      <Edit Name="prpToneAveragerCruiseTimeScale" Caption="Cruise averager scale" X="2" Width="246" Height="22" CaptionWidth="150" Help="Time scale used for cruise speed command averager.">
        <DataField DataType="enum"/>
      </Edit>
  
      <Edit Name="prpToneMeanVolumeCircling" Caption="Circling volume" X="2" Width="246" Height="22" CaptionWidth="150" Help="Mean volume level in circling modes.">
        <DataField DataType="double" DisplayFormat="%.0f/8" EditFormat="%.0f" Min="0" Max="8" Step="1"/>
      </Edit>
    
      <Edit Name="prpToneMeanVolumeCruise" Caption="Cruise volume" X="2" Width="246" Height="22" CaptionWidth="150" Help="Mean volume level in cruise modes.  If set to zero, scales with airspeed.">
        <DataField DataType="double" DisplayFormat="%.0f/8" EditFormat="%.0f" Min="0" Max="8" Step="1"/>
      </Edit>
    
      <Edit Name="prpToneBaseFrequencyOffset" Caption="Base frequency" X="2" Width="246" Height="22" CaptionWidth="150" Help="Adjustment to base frequency of tones in all modes.">
        <DataField DataType="double" DisplayFormat="%.0f" EditFormat="%.0f" Min="-30" Max="30" Step="1"/>
      </Edit>
  
      <Edit Name="prpVarioTimeConstantCircling" Caption="Filter circling" X="2" Width="246" Height="22" CaptionWidth="150" Help="Variometer low pass filter time constant in circling mode.">
        <DataField DataType="enum"/>
      </Edit>
  
      <Edit Name="prpVarioTimeConstantCruise" Caption="Filter cruise" X="2" Width="246" Height="22" CaptionWidth="150" Help="Variometer low pass filter time constant in cruise mode.">
        <DataField DataType="enum"/>
      </Edit>
  
      <Edit Name="prpTonePitchScale" Caption="Tone pitch scale" X="2" Width="246" Height="22" CaptionWidth="150" Help="Adjustment to base pitch scale of tones in all modes.">
        <DataField DataType="double" DisplayFormat="%.0f" EditFormat="%.0f" Min="32" Max="100" Step="1"/>
      </Edit>
    </Panel>
  
