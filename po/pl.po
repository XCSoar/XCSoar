#: src/InfoBoxes/Content/Factory.cpp:695
#, no-c-format
msgid "AAT speed around target"
msgstr "AAT V PT: Średnia prędkość wg pozostałych PZT"

#: src/InfoBoxes/Content/Factory.cpp:696
#, no-c-format
msgid "AAT Vtgt"
msgstr "AAT V PT"

#: src/InfoBoxes/Content/Factory.cpp:697
#, no-c-format
msgid ""
